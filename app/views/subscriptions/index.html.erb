<h5 aria-label="breadcrumb">
  <ol class="breadcrumb">
    <li class="breadcrumb-item"><a href="<%= root_path %>"
                                class="link-dark">Home</a></li>
    <li class="breadcrumb-item active text-secondary" aria-current="page">Planos de Assinatura</li>
  </ol>
</h5>

<hr>
<% @subscriptions.each do |subscription| %>
  <div id="<%= subscription.id %>">
    <%= link_to subscription.name, subscription %><br>
    <%= subscription.description %>
    <hr>
  </div>
<% end %>
