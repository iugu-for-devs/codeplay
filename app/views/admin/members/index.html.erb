<h5 aria-label="breadcrumb">
  <ol class="breadcrumb">
    <li class="breadcrumb-item"><a href="<%= admin_root_path %>"class="link-dark"><%=t('breadcrumb.dashboard_admin')%></a></li>
    <li class="breadcrumb-item active text-secondary" aria-current="page"><%=t('breadcrumb.member_managment')%></li>
  </ol>
</h5>

<div class="btn-group">
  <%= link_to t('buttons.register', model: Admin.model_name.human), new_admin_member_path, class: 'btn btn-primary' %>
</div>

<p class="h5 mb-3 fw-normal text-center"><%= t('label.list', model: Admin.model_name.human(count: :other)) %></p>

<div>
  <table class="table table-bordered">
    <thead class="table-dark">
      <tr>
        <th scope="col"><%= Admin.human_attribute_name(:name) %></th>
        <th scope="col"><%= Admin.human_attribute_name(:email) %></th>
      </tr>
    </thead>
    <tbody>
      <% @members.each do |member| %>
        <tr>
          <td><%= member.name %></td>
          <td><%= member.email %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
